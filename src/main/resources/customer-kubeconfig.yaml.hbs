apiVersion: v1
kind: Config
users:
- name: {{serviceAccountName}}
  user:
    token: {{token}}
clusters:
- cluster:
    certificate-authority-data: {{certificateAuthorityData}}
    server: {{server}}
  name: customer-cluster
contexts:
- context:
    cluster: customer-cluster
    user: {{serviceAccountName}}
  name: customer-cluster-context
current-context: customer-cluster-context
